{
  "name": "chess-fen-to-jpeg",
  "version": "1.0.0",
  "description": "A tool that will convert a set of moves to a image",
  "main": "src/index.ts",
  "author": "me505",
  "license": "MIT",
  "engines": {
    "node": ">=20.10.0"
  },
  "type": "module",
  "scripts": {
    "start": "tsx src/main.ts",
    "register": "tsx src/register.ts",
    "format": "run-s format:prettier format:lint",
    "format:lint": "eslint --fix .",
    "format:prettier": "prettier --write .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.32.4",
    "canvas": "^2.11.2",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "jimp": "^0.22.10",
    "lowdb": "^7.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "esbuild": "^0.19.8",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "knip": "^3.3.0",
    "lint-staged": "^15.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.2"
  },
  "lint-staged": {
    "*.ts": [
      "yarn prettier --write",
      "eslint --fix"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
